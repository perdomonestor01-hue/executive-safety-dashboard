{
  "name": "executive-safety-dashboard",
  "version": "1.0.0",
  "description": "Enterprise-grade executive safety dashboard for comprehensive safety analytics and reporting",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "webpack --mode production",
    "test": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:smoke": "jest --config jest.smoke.config.js",
    "test:health": "node scripts/health-check.js",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "docker:build": "docker build -t executive-safety-dashboard .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js"
  },
  "keywords": [
    "safety",
    "dashboard",
    "executive",
    "analytics",
    "workers-compensation",
    "enterprise",
    "docker",
    "kubernetes"
  ],
  "author": "JuFiPAi",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "knex": "^3.1.0",
    "redis": "^4.6.10",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-azure-ad": "^4.3.5",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "axios": "^1.6.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "socket.io": "^4.7.4",
    "express-validator": "^7.0.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "dotenv": "^16.3.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "prometheus-api-metrics": "^3.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-loader": "^9.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/perdomonestor01-hue/executive-safety-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/perdomonestor01-hue/executive-safety-dashboard/issues"
  },
  "homepage": "https://github.com/perdomonestor01-hue/executive-safety-dashboard#readme"
}